####
### launch_show_hide applications
####

# launch_show_hide Strongbox
ctrl + shift - z : ~/.config/skhd/launch_show_hide.sh Strongbox;

# launch_show_hide Spotify
ctrl + shift - s : ~/.config/skhd/launch_show_hide.sh Spotify;

# launch_show_hide Brave
ctrl + shift - b : ~/.config/skhd/launch_show_hide.sh Brave;

# launch_show_hide iTerm
ctrl - space : ~/.config/skhd/launch_show_hide.sh iTerm


